        /* CSS Variables for Theme Colors */
        :root {
            --primary-red: #d32f2f; /* Adjusted red for better contrast */
            --primary-blue: #3B82F6;
            --dark-blue: #1e40af;
            --light-blue: #dbeafe;
            --light-red: #ffebee;
            --text-dark: #1e293b;
            --text-light: #64748b;
            --background: #f8fafc;
            --white: #ffffff;
            --border: #e2e8f0;
            --shadow: rgba(0, 0, 0, 0.08);
            --shadow-hover: rgba(0, 0, 0, 0.12);
        }

        /* Base Styles */
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }

        body {
            background-color: var(--background);
            color: var(--text-dark);
            line-height: 1.6;
        
        }

        .container {
            max-width: 90%;
            margin: 0 auto;
           
        }

        /* Header Styles - Assuming you have your own header */
        .header-placeholder {
            background: linear-gradient(135deg, var(--primary-red) 0%, var(--primary-blue) 100%);
            color: white;
            padding: 30px 0;
            text-align: center;
            margin-bottom: 40px;
            border-radius: 0 0 10px 10px;
            box-shadow: 0 4px 12px var(--shadow);
        }

        .header-placeholder h1 {
            font-size: 2.5rem;
            margin-bottom: 10px;
            font-weight: 700;
        }

        .header-placeholder p {
            font-size: 1.1rem;
            opacity: 0.9;
            max-width: 800px;
            margin: 0 auto;
        }

        /* Main Content */
        .terms-wrapper {
            display: flex;
            flex-direction: column;
            gap: 30px;
        }

        .page-title {
            text-align: center;
            margin-bottom: 20px;
            position: relative;
            padding-bottom: 15px;
            margin-top: 3rem;
        }

        .page-title h2 {
            font-size: 2.5rem;
            color: var(--text-dark);
            margin-bottom: 10px;
        }

        .page-title h2::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 50%;
            transform: translateX(-50%);
            width: 100px;
            height: 4px;
            background: linear-gradient(to right, var(--primary-red), var(--primary-blue));
            border-radius: 2px;
        }

        .page-title p {
            color: var(--text-light);
            font-size: 1.8rem;
            max-width: 800px;
            margin: 0 auto;
        }

        /* Content Box */
        .content-box {
            background: var(--white);
            border-radius: 12px;
            box-shadow: 0 6px 15px var(--shadow);
            padding: 40px;
            margin-bottom: 20px;
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

        .content-box:hover {
            box-shadow: 0 10px 25px var(--shadow-hover);
        }

        /* Section Styles */
        .terms-section {
            margin-bottom: 40px;
            scroll-margin-top: 20px;
        }

        .terms-section:last-child {
            margin-bottom: 0;
        }

        .section-title {
            display: flex;
            align-items: center;
            margin-bottom: 20px;
            padding-bottom: 12px;
            border-bottom: 2px solid var(--border);
            position: relative;
        }

        .section-title::after {
            content: '';
            position: absolute;
            bottom: -2px;
            left: 0;
            width: 80px;
            height: 2px;
            background: linear-gradient(to right, var(--primary-red), var(--primary-blue));
        }

        .section-title i {
            background: linear-gradient(135deg, var(--primary-red), var(--primary-blue));
            -webkit-background-clip: text;
            background-clip: text;
            color: transparent;
            font-size: 1.8rem;
            margin-right: 15px;
            width: 40px;
            text-align: center;
        }

        .section-title h3 {
            font-size: 1.6rem;
            color: var(--text-dark);
            font-weight: 600;
        }

        .section-content {
            padding-left: 55px;
        }

        .section-content p {
            margin-bottom: 15px;
            color: var(--text-dark);
        }

        .section-content ul, .section-content ol {
            margin-bottom: 20px;
            padding-left: 20px;
        }

        .section-content li {
            margin-bottom: 10px;
            color: var(--text-dark);
            position: relative;
            padding-left: 5px;
        }

        .highlight-box {
            background: var(--light-blue);
            border-left: 4px solid var(--primary-blue);
            padding: 20px;
            margin: 20px 0;
            border-radius: 0 8px 8px 0;
        }

        .warning-box {
            background: var(--light-red);
            border-left: 4px solid var(--primary-red);
            padding: 20px;
            margin: 20px 0;
            border-radius: 0 8px 8px 0;
        }

        .highlight-box h4, .warning-box h4 {
            margin-bottom: 10px;
            display: flex;
            align-items: center;
        }

        .highlight-box h4 i {
            color: var(--primary-blue);
            margin-right: 10px;
        }

        .warning-box h4 i {
            color: var(--primary-red);
            margin-right: 10px;
        }

        /* Product Reference Section */
        .product-reference {
            background: linear-gradient(135deg, #f0f9ff 0%, #fef2f2 100%);
            border-radius: 12px;
            padding: 25px;
            margin: 25px 0;
            border: 1px solid var(--border);
        }

        .product-reference h4 {
            color: var(--primary-blue);
            margin-bottom: 15px;
            font-size: 1.3rem;
            display: flex;
            align-items: center;
        }

        .product-reference h4 i {
            margin-right: 10px;
        }

        .product-reference p {
            color: var(--text-dark);
        }

        /* Navigation Tabs for Quick Access */
        .quick-nav {
            display: flex;
            flex-wrap: wrap;
            gap: 12px;
            margin-bottom: 30px;
            justify-content: center;
        }

        .nav-tab {
            background: var(--white);
            padding: 12px 20px;
            border-radius: 50px;
            box-shadow: 0 3px 8px var(--shadow);
            text-decoration: none;
            color: var(--text-dark);
            font-weight: 500;
            transition: all 0.3s ease;
            border: 1px solid transparent;
            display: flex;
            align-items: center;
        }

        .nav-tab i {
            margin-right: 8px;
            font-size: 0.9rem;
        }

        .nav-tab:hover {
            transform: translateY(-3px);
            box-shadow: 0 6px 12px var(--shadow-hover);
            color: var(--primary-blue);
            border-color: var(--primary-blue);
        }

        .nav-tab.red:hover {
            color: var(--primary-red);
            border-color: var(--primary-red);
        }

        /* Important Notice */
        .important-notice {
            background: linear-gradient(135deg, var(--primary-red) 0%, #b91c1c 100%);
            color: white;
            padding: 25px;
            border-radius: 12px;
            margin: 30px 0;
            text-align: center;
            box-shadow: 0 6px 15px rgba(211, 47, 47, 0.2);
        }

        .important-notice h3 {
            font-size: 1.5rem;
            margin-bottom: 10px;
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
        }

        .important-notice h3 i {
            margin-right: 10px;
        }

        /* Footer Styles - Assuming you have your own footer */
        .footer-placeholder {
            background: var(--text-dark);
            color: var(--white);
            padding: 30px 0;
            text-align: center;
            margin-top: 60px;
            border-radius: 10px 10px 0 0;
        }

        .footer-placeholder p {
            margin-bottom: 10px;
        }

        .footer-links {
            display: flex;
            justify-content: center;
            flex-wrap: wrap;
            gap: 20px;
            margin-top: 20px;
        }

        .footer-links a {
            color: var(--light-blue);
            text-decoration: none;
            transition: color 0.3s;
        }

        .footer-links a:hover {
            color: var(--white);
            text-decoration: underline;
        }

        /* Responsive Design */
        @media (max-width: 992px) {
            .content-box {
                padding: 30px;
            }
            
            .section-content {
                padding-left: 0;
            }
            
            .section-title {
                flex-direction: column;
                align-items: flex-start;
            }
            
            .section-title i {
                margin-bottom: 10px;
            }
        }

        @media (max-width: 768px) {
            .header-placeholder h1 {
                font-size: 2rem;
            }
            
            .page-title h2 {
                font-size: 2.5rem;
            }
            
            .content-box {
                padding: 25px 20px;
            }
            
            .section-title h3 {
                font-size: 1.4rem;
            }
            
            .quick-nav {
                flex-direction: column;
                align-items: center;
            }
            
            .nav-tab {
                width: 100%;
                justify-content: center;
            }
        }

        @media (max-width: 480px) {
            .header-placeholder {
                padding: 20px 0;
            }
            
            .header-placeholder h1 {
                font-size: 1.7rem;
            }
            
            .page-title h2 {
                font-size: 2rem;
            }
            
            .section-title h3 {
                font-size: 1.3rem;
            }
        }

        /* Print Styles */
        @media print {
            .quick-nav, .header-placeholder, .footer-placeholder {
                display: none;
            }
            
            .content-box {
                box-shadow: none;
                border: 1px solid #ddd;
            }
        }
    </style>
</head>

