    <!-- services details area start -->
    <section class="rs-services-area section-space ">
      <div class="container">
        <div class="row g-5">
          <!-- Main content -->
          <div class="col-xl-8 col-lg-8">
            <div class="rs-services-details-wrapper">

              <!-- Product image gallery (from database) -->
              <div class="rs-services-details-thumb">
                <img
                  id="main-product-image"
                  src="<%= product && product.images && product.images.length ? ('/uploads/' + product.images[0]) : '/images/teraxo/prd-2.webp' %>"
                  alt="<%= product && product.title ? product.title : 'Product Image' %>">
              </div>
              <div class="product-gallery">
                <div class="swiper product-gallery-swiper">
                  <div class="swiper-wrapper">
                    <% if (product && product.images && product.images.length) { %>
                      <% product.images.forEach(function(img, idx) { %>
                        <div class="swiper-slide gallery-item <%= idx === 0 ? 'active' : '' %>" data-image="/uploads/<%= img %>">
                          <img src="/uploads/<%= img %>" alt="<%= product.title %> - View <%= idx + 1 %>">
                        </div>
                      <% }) %>
                    <% } else { %>
                      <!-- fallback static -->
                      <div class="swiper-slide gallery-item active" data-image="/images/teraxo/prd-1.jpg">
                        <img src="/images/teraxo/prd-1.jpg" alt="Product - View 1">
                      </div>
                    <% } %>
                  </div>
                  <div class="swiper-pagination"></div>
                </div>
              </div>

              <!-- Title / Overview (from database) -->
              <h3 class="rs-services-details-title mt-50"><%= product && product.title ? product.title : 'Product' %></h3>
              <% if (product && product.subDescription) { %>
                <p class="mb-25 mt-15"><strong><%= product.subDescription %></strong></p>
              <% } else { %>
                <p class="mb-25 mt-15"><strong>FX40 is Teraxo’s premium instant adhesive engineered for sign-making, PVC/UPVC, acrylic and other display applications where a clean, non-whitening finish is essential.</strong></p>
              <% } %>

              <!-- Description (from database) -->
              <% if (product && product.description) { %>
                <p><%= product.description %></p>
              <% } else { %>
                <p>
                  FX40 is a fast-curing ethyl cyanoacrylate adhesive formulated to deliver instant grip, high bond
                  strength and a crystal-clear finish. Its advanced anti-whitening formula prevents bloom/whitening on
                  glossy, matte and textured surfaces—making it ideal for signage, acrylic fabrication and interior
                  display work.
                </p>
